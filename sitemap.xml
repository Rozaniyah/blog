<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">{% for post in site.posts %}
<url><loc>{{ post.url | absolute_url }}</loc></url>{% endfor %}
{% for page in site.pages %}{% if page.url == '/search.json' or page.url == '/sitemap.xml' or page.url == '/404.html' %} {% continue %} {% endif %}
<url><loc>{{ page.url | replace:'/index.html','/' | absolute_url }}</loc></url>{% endfor %}
</urlset>